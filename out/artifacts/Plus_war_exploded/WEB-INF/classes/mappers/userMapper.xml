<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.plus.server.dal.user">
    <insert id="insertUser" parameterType="com.plus.server.model.User">
        insert into user(phone,email,password_hash,password_salt,valid,now(),now())
        values (#{phone},#{email},#{password_hash},#{password_salt},#{valid})
    </insert>
</mapper>